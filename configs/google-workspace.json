{
  "services": {
    "google": {
      "name": "Google Workspace",
      "baseURL": "https://www.googleapis.com",
      "auth": {
        "type": "oauth2_external",
        "config": {
          "clientId": "${GOOGLE_CLIENT_ID}",
          "clientSecret": "${GOOGLE_CLIENT_SECRET}",
          "scope": "https://www.googleapis.com/auth/userinfo.email https://www.googleapis.com/auth/userinfo.profile https://www.googleapis.com/auth/calendar https://www.googleapis.com/auth/gmail.modify https://www.googleapis.com/auth/drive https://www.googleapis.com/auth/contacts.readonly",
          "tokenURL": "https://oauth2.googleapis.com/token"
        },
        "tokenInvalidation": {
          "statusCodes": [401],
          "retryOnInvalidation": true
        }
      },
      "retry": {
        "enabled": true,
        "maxAttempts": 3,
        "strategy": "exponential",
        "baseDelay": "1s",
        "maxDelay": "30s",
        "jitter": true,
        "backoffFactor": 2.0,
        "retryableErrors": ["network_error", "timeout", "server_error", "rate_limited"]
      },
      "circuitBreaker": {
        "enabled": true,
        "failureThreshold": 5,
        "successThreshold": 3,
        "timeout": "30s",
        "halfOpenMaxCalls": 3,
        "resetTimeout": "60s"
      },
      "endpoints": [
        {
          "id": "profile_get",
          "name": "Get User Profile",
          "description": "Get the current user's profile information from Google",
          "method": "GET",
          "path": "/oauth2/v2/userinfo",
          "parameters": [],
          "response": {
            "type": "json",
            "caching": {
              "enabled": true,
              "ttl": "30m"
            }
          }
        },
        {
          "id": "calendar_events_list",
          "name": "List Calendar Events",
          "description": "List events from Google Calendar",
          "method": "GET",
          "path": "/calendar/v3/calendars/primary/events",
          "parameters": [
            {
              "name": "startDate",
              "description": "Start date in YYYYMMDD format",
              "type": "string",
              "required": false,
              "location": "query",
              "validation": {
                "pattern": "^\\d{8}$"
              },
              "transform": {
                "targetName": "timeMin",
                "expression": "concat(slice(0,4), '-', slice(4,6), '-', slice(6,8), 'T00:00:00Z')"
              }
            },
            {
              "name": "endDate",
              "description": "End date in YYYYMMDD format",
              "type": "string",
              "required": false,
              "location": "query",
              "validation": {
                "pattern": "^\\d{8}$"
              },
              "transform": {
                "targetName": "timeMax",
                "expression": "concat(slice(0,4), '-', slice(4,6), '-', slice(6,8), 'T23:59:59Z')"
              }
            },
            {
              "name": "maxResults",
              "description": "Maximum number of events returned (1-2500)",
              "type": "number",
              "required": false,
              "location": "query",
              "default": 10,
              "validation": {
                "minLength": 1,
                "maxLength": 2500
              }
            },
            {
              "name": "singleEvents",
              "description": "Whether to expand recurring events",
              "type": "boolean",
              "required": false,
              "location": "query",
              "default": true
            },
            {
              "name": "orderBy",
              "description": "Order of events returned",
              "type": "string",
              "required": false,
              "location": "query",
              "default": "startTime",
              "validation": {
                "enum": ["startTime", "updated"]
              }
            }
          ],
          "response": {
            "type": "json",
            "paginated": true,
            "paginationConfig": {
              "nextPageTokenPath": "nextPageToken",
              "dataPath": "items",
              "pageSize": 250
            },
            "caching": {
              "enabled": true,
              "ttl": "5m"
            },
            "transform": ".items | map({id: .id, summary: .summary, start: .start.dateTime // .start.date, end: .end.dateTime // .end.date, location: .location, description: .description})"
          }
        },
        {
          "id": "calendar_event_create",
          "name": "Create Calendar Event",
          "description": "Create a new event in Google Calendar",
          "method": "POST",
          "path": "/calendar/v3/calendars/primary/events",
          "parameters": [
            {
              "name": "summary",
              "description": "Event title/summary",
              "type": "string",
              "required": true,
              "location": "body"
            },
            {
              "name": "description",
              "description": "Event description",
              "type": "string",
              "required": false,
              "location": "body"
            },
            {
              "name": "location",
              "description": "Event location",
              "type": "string",
              "required": false,
              "location": "body"
            },
            {
              "name": "start",
              "description": "Event start time object, e.g. {\"dateTime\": \"2025-01-15T10:00:00Z\", \"timeZone\": \"UTC\"}",
              "type": "object",
              "required": true,
              "location": "body"
            },
            {
              "name": "end",
              "description": "Event end time object, e.g. {\"dateTime\": \"2025-01-15T11:00:00Z\", \"timeZone\": \"UTC\"}",
              "type": "object",
              "required": true,
              "location": "body"
            }
          ],
          "response": {
            "type": "json"
          },
          "retry": {
            "enabled": true,
            "maxAttempts": 5,
            "strategy": "exponential",
            "baseDelay": "2s",
            "maxDelay": "60s",
            "jitter": true,
            "backoffFactor": 2.0,
            "retryableErrors": ["network_error", "timeout", "server_error", "rate_limited"]
          }
        },
        {
          "id": "calendar_event_get",
          "name": "Get Calendar Event",
          "description": "Get a specific calendar event by ID",
          "method": "GET",
          "path": "/calendar/v3/calendars/primary/events/{eventId}",
          "parameters": [
            {
              "name": "eventId",
              "description": "The ID of the event to retrieve",
              "type": "string",
              "required": true,
              "location": "path"
            }
          ],
          "response": {
            "type": "json",
            "caching": {
              "enabled": true,
              "ttl": "10m"
            }
          }
        },
        {
          "id": "calendar_event_update",
          "name": "Update Calendar Event",
          "description": "Update an existing calendar event. Only provide the fields you want to change.",
          "method": "PATCH",
          "path": "/calendar/v3/calendars/primary/events/{eventId}",
          "parameters": [
            {
              "name": "eventId",
              "description": "The ID of the event to update",
              "type": "string",
              "required": true,
              "location": "path"
            },
            {
              "name": "summary",
              "description": "Event title/summary",
              "type": "string",
              "required": false,
              "location": "body"
            },
            {
              "name": "description",
              "description": "Event description",
              "type": "string",
              "required": false,
              "location": "body"
            },
            {
              "name": "location",
              "description": "Event location",
              "type": "string",
              "required": false,
              "location": "body"
            },
            {
              "name": "start",
              "description": "Event start time object, e.g. {\"dateTime\": \"2025-01-15T10:00:00Z\", \"timeZone\": \"UTC\"}",
              "type": "object",
              "required": false,
              "location": "body"
            },
            {
              "name": "end",
              "description": "Event end time object, e.g. {\"dateTime\": \"2025-01-15T11:00:00Z\", \"timeZone\": \"UTC\"}",
              "type": "object",
              "required": false,
              "location": "body"
            }
          ],
          "response": {
            "type": "json"
          }
        },
        {
          "id": "calendar_search",
          "name": "Search Calendar Events",
          "description": "Search calendar events by keyword within a date range. The query parameter matches against event summary, description, location, attendees, and other fields.",
          "method": "GET",
          "path": "/calendar/v3/calendars/primary/events",
          "parameters": [
            {
              "name": "q",
              "description": "Free text search terms to find events matching summary, description, location, or attendees",
              "type": "string",
              "required": true,
              "location": "query"
            },
            {
              "name": "startDate",
              "description": "Start date in YYYYMMDD format to bound the search",
              "type": "string",
              "required": false,
              "location": "query",
              "validation": {
                "pattern": "^\\d{8}$"
              },
              "transform": {
                "targetName": "timeMin",
                "expression": "concat(slice(0,4), '-', slice(4,6), '-', slice(6,8), 'T00:00:00Z')"
              }
            },
            {
              "name": "endDate",
              "description": "End date in YYYYMMDD format to bound the search",
              "type": "string",
              "required": false,
              "location": "query",
              "validation": {
                "pattern": "^\\d{8}$"
              },
              "transform": {
                "targetName": "timeMax",
                "expression": "concat(slice(0,4), '-', slice(4,6), '-', slice(6,8), 'T23:59:59Z')"
              }
            },
            {
              "name": "maxResults",
              "description": "Maximum number of events to return (1-2500)",
              "type": "number",
              "required": false,
              "location": "query",
              "default": 25,
              "validation": {
                "minimum": 1,
                "maximum": 2500
              }
            },
            {
              "name": "singleEvents",
              "description": "Whether to expand recurring events",
              "type": "boolean",
              "required": false,
              "location": "query",
              "default": true
            },
            {
              "name": "orderBy",
              "description": "Order of events returned",
              "type": "string",
              "required": false,
              "location": "query",
              "default": "startTime",
              "validation": {
                "enum": ["startTime", "updated"]
              }
            }
          ],
          "response": {
            "type": "json",
            "transform": ".items | map({id: .id, summary: .summary, start: .start.dateTime // .start.date, end: .end.dateTime // .end.date, location: .location, description: .description, attendees: .attendees})"
          }
        },
        {
          "id": "calendar_list",
          "name": "List Calendars",
          "description": "List all calendars the user has access to (primary, secondary, shared)",
          "method": "GET",
          "path": "/calendar/v3/users/me/calendarList",
          "parameters": [
            {
              "name": "maxResults",
              "description": "Maximum number of calendars to return (1-250)",
              "type": "number",
              "required": false,
              "location": "query",
              "default": 100,
              "validation": {
                "minimum": 1,
                "maximum": 250
              }
            },
            {
              "name": "showHidden",
              "description": "Whether to show hidden calendars",
              "type": "boolean",
              "required": false,
              "location": "query",
              "default": false
            },
            {
              "name": "showDeleted",
              "description": "Whether to include deleted calendars",
              "type": "boolean",
              "required": false,
              "location": "query",
              "default": false
            }
          ],
          "response": {
            "type": "json",
            "paginated": true,
            "paginationConfig": {
              "nextPageTokenPath": "nextPageToken",
              "dataPath": "items",
              "pageSize": 250
            },
            "transform": ".items | map({id: .id, summary: .summary, description: .description, primary: .primary, accessRole: .accessRole, backgroundColor: .backgroundColor})"
          }
        },
        {
          "id": "gmail_messages_list",
          "name": "List Gmail Messages",
          "description": "List messages from Gmail inbox",
          "method": "GET",
          "path": "/gmail/v1/users/me/messages",
          "parameters": [
            {
              "name": "q",
              "description": "Gmail search query (e.g., 'is:unread', 'from:example@gmail.com')",
              "type": "string",
              "required": false,
              "location": "query"
            },
            {
              "name": "labelIds",
              "description": "Label IDs to filter by (comma-separated)",
              "type": "string",
              "required": false,
              "location": "query",
              "default": "INBOX"
            },
            {
              "name": "maxResults",
              "description": "Maximum number of messages to return (1-500)",
              "type": "number",
              "required": false,
              "location": "query",
              "default": 10,
              "validation": {
                "minLength": 1,
                "maxLength": 500
              }
            },
            {
              "name": "includeSpamTrash",
              "description": "Include messages from SPAM and TRASH",
              "type": "boolean",
              "required": false,
              "location": "query",
              "default": false
            }
          ],
          "response": {
            "type": "json",
            "paginated": true,
            "paginationConfig": {
              "nextPageTokenPath": "nextPageToken",
              "dataPath": "messages",
              "pageSize": 100
            },
            "caching": {
              "enabled": true,
              "ttl": "2m"
            }
          }
        },
        {
          "id": "gmail_message_get",
          "name": "Get Gmail Message Summary",
          "description": "Get a specific Gmail message by ID with headers (Subject, From, To, Date), labels, and snippet. Use gmail_message_read for full content.",
          "method": "GET",
          "path": "/gmail/v1/users/me/messages/{messageId}",
          "parameters": [
            {
              "name": "messageId",
              "description": "The ID of the message to retrieve",
              "type": "string",
              "required": true,
              "location": "path"
            },
            {
              "name": "format",
              "type": "string",
              "location": "query",
              "static": true,
              "default": "full"
            }
          ],
          "response": {
            "type": "json",
            "caching": {
              "enabled": true,
              "ttl": "15m"
            },
            "transform": "{id: .id, threadId: .threadId, labelIds: .labelIds, snippet: .snippet, internalDate: .internalDate, sizeEstimate: .sizeEstimate, headers: [.payload.headers[] | select(.name | test(\"^(Subject|From|To|Date|Cc)$\"; \"i\"))]}"
          }
        },
        {
          "id": "gmail_message_read",
          "name": "Read Gmail Message",
          "description": "Read a single email message by ID with full content including body and attachments",
          "method": "GET",
          "path": "/gmail/v1/users/me/messages/{messageId}",
          "parameters": [
            {
              "name": "messageId",
              "description": "The ID of the message to retrieve",
              "type": "string",
              "required": true,
              "location": "path"
            },
            {
              "name": "format",
              "description": "Format of the message returned (default: full)",
              "type": "string",
              "required": false,
              "location": "query",
              "default": "full",
              "validation": {
                "enum": ["minimal", "full", "raw", "metadata"]
              }
            }
          ],
          "response": {
            "type": "json",
            "caching": {
              "enabled": true,
              "ttl": "15m"
            }
          }
        },
        {
          "id": "gmail_draft_create",
          "name": "Create Gmail Draft",
          "description": "Create a new draft email message. The draft is saved but NOT sent.",
          "method": "POST",
          "path": "/gmail/v1/users/me/drafts",
          "parameters": [
            {
              "name": "to",
              "description": "Recipient email address",
              "type": "string",
              "required": true,
              "location": "body"
            },
            {
              "name": "subject",
              "description": "Email subject line",
              "type": "string",
              "required": true,
              "location": "body"
            },
            {
              "name": "body",
              "description": "Email body content (plain text)",
              "type": "string",
              "required": true,
              "location": "body"
            },
            {
              "name": "cc",
              "description": "CC recipients (comma-separated)",
              "type": "string",
              "required": false,
              "location": "body"
            },
            {
              "name": "bcc",
              "description": "BCC recipients (comma-separated)",
              "type": "string",
              "required": false,
              "location": "body"
            }
          ],
          "response": {
            "type": "json"
          }
        },
        {
          "id": "gmail_draft_get",
          "name": "Get Gmail Draft",
          "description": "Get a specific draft email message by ID",
          "method": "GET",
          "path": "/gmail/v1/users/me/drafts/{draftId}",
          "parameters": [
            {
              "name": "draftId",
              "description": "The ID of the draft to retrieve",
              "type": "string",
              "required": true,
              "location": "path"
            },
            {
              "name": "format",
              "description": "Format of the returned message",
              "type": "string",
              "required": false,
              "location": "query",
              "default": "full",
              "validation": {
                "enum": ["minimal", "full", "raw", "metadata"]
              }
            }
          ],
          "response": {
            "type": "json"
          }
        },
        {
          "id": "gmail_draft_update",
          "name": "Update Gmail Draft",
          "description": "Update an existing draft email message. Only provide the fields you want to change. The draft is saved but NOT sent.",
          "method": "PUT",
          "path": "/gmail/v1/users/me/drafts/{draftId}",
          "parameters": [
            {
              "name": "draftId",
              "description": "The ID of the draft to update",
              "type": "string",
              "required": true,
              "location": "path"
            },
            {
              "name": "to",
              "description": "Updated recipient email address",
              "type": "string",
              "required": false,
              "location": "body"
            },
            {
              "name": "subject",
              "description": "Updated email subject line",
              "type": "string",
              "required": false,
              "location": "body"
            },
            {
              "name": "body",
              "description": "Updated email body content (plain text)",
              "type": "string",
              "required": false,
              "location": "body"
            },
            {
              "name": "cc",
              "description": "Updated CC recipients (comma-separated)",
              "type": "string",
              "required": false,
              "location": "body"
            },
            {
              "name": "bcc",
              "description": "Updated BCC recipients (comma-separated)",
              "type": "string",
              "required": false,
              "location": "body"
            }
          ],
          "response": {
            "type": "json"
          }
        },
        {
          "id": "gmail_draft_delete",
          "name": "Delete Gmail Draft",
          "description": "Permanently delete a draft email message. This action cannot be undone.",
          "method": "DELETE",
          "path": "/gmail/v1/users/me/drafts/{draftId}",
          "parameters": [
            {
              "name": "draftId",
              "description": "The ID of the draft to delete",
              "type": "string",
              "required": true,
              "location": "path"
            }
          ],
          "response": {
            "type": "json"
          }
        },
        {
          "id": "gmail_draft_list",
          "name": "List Gmail Drafts",
          "description": "List draft email messages in Gmail",
          "method": "GET",
          "path": "/gmail/v1/users/me/drafts",
          "parameters": [
            {
              "name": "maxResults",
              "description": "Maximum number of drafts to return (1-500)",
              "type": "number",
              "required": false,
              "location": "query",
              "default": 25,
              "validation": {
                "minimum": 1,
                "maximum": 500
              }
            },
            {
              "name": "q",
              "description": "Gmail search query to filter drafts (e.g., 'subject:report', 'to:user@example.com')",
              "type": "string",
              "required": false,
              "location": "query"
            },
            {
              "name": "pageToken",
              "description": "Page token for pagination",
              "type": "string",
              "required": false,
              "location": "query"
            }
          ],
          "response": {
            "type": "json",
            "paginated": true,
            "paginationConfig": {
              "nextPageTokenPath": "nextPageToken",
              "dataPath": "drafts",
              "pageSize": 100
            }
          }
        },
        {
          "id": "gmail_draft_reply",
          "name": "Create Gmail Draft Reply",
          "description": "Create a draft reply to an existing message, preserving threading and quoted content. The draft is saved but NOT sent.",
          "method": "POST",
          "path": "/gmail/v1/users/me/drafts",
          "parameters": [
            {
              "name": "messageId",
              "description": "The ID of the message to reply to. Used to set the threadId and In-Reply-To/References headers.",
              "type": "string",
              "required": true,
              "location": "body",
              "transform": {
                "targetName": "message.threadId",
                "expression": "."
              }
            },
            {
              "name": "to",
              "description": "Recipient email address (typically the original sender)",
              "type": "string",
              "required": true,
              "location": "body"
            },
            {
              "name": "subject",
              "description": "Reply subject line (typically 'Re: <original subject>')",
              "type": "string",
              "required": true,
              "location": "body"
            },
            {
              "name": "body",
              "description": "Reply body content (plain text)",
              "type": "string",
              "required": true,
              "location": "body"
            },
            {
              "name": "cc",
              "description": "CC recipients (comma-separated)",
              "type": "string",
              "required": false,
              "location": "body"
            }
          ],
          "response": {
            "type": "json"
          }
        },
        {
          "id": "gmail_draft_reply_all",
          "name": "Create Gmail Draft Reply All",
          "description": "Create a draft reply-all to an existing message, preserving threading and quoted content. Include all original recipients in the to and cc fields. The draft is saved but NOT sent.",
          "method": "POST",
          "path": "/gmail/v1/users/me/drafts",
          "parameters": [
            {
              "name": "messageId",
              "description": "The ID of the message to reply to. Used to set the threadId and In-Reply-To/References headers.",
              "type": "string",
              "required": true,
              "location": "body",
              "transform": {
                "targetName": "message.threadId",
                "expression": "."
              }
            },
            {
              "name": "to",
              "description": "To recipients (comma-separated, include original sender and To recipients)",
              "type": "string",
              "required": true,
              "location": "body"
            },
            {
              "name": "subject",
              "description": "Reply subject line (typically 'Re: <original subject>')",
              "type": "string",
              "required": true,
              "location": "body"
            },
            {
              "name": "body",
              "description": "Reply body content (plain text)",
              "type": "string",
              "required": true,
              "location": "body"
            },
            {
              "name": "cc",
              "description": "CC recipients (comma-separated, include original CC recipients)",
              "type": "string",
              "required": false,
              "location": "body"
            },
            {
              "name": "bcc",
              "description": "BCC recipients (comma-separated)",
              "type": "string",
              "required": false,
              "location": "body"
            }
          ],
          "response": {
            "type": "json"
          }
        },
        {
          "id": "gmail_draft_forward",
          "name": "Create Gmail Draft Forward",
          "description": "Create a draft forward of an existing message to specified recipients. The draft is saved but NOT sent.",
          "method": "POST",
          "path": "/gmail/v1/users/me/drafts",
          "parameters": [
            {
              "name": "messageId",
              "description": "The ID of the message to forward. Used to set the threadId.",
              "type": "string",
              "required": true,
              "location": "body",
              "transform": {
                "targetName": "message.threadId",
                "expression": "."
              }
            },
            {
              "name": "to",
              "description": "Recipient email address to forward to",
              "type": "string",
              "required": true,
              "location": "body"
            },
            {
              "name": "subject",
              "description": "Forward subject line (typically 'Fwd: <original subject>')",
              "type": "string",
              "required": true,
              "location": "body"
            },
            {
              "name": "body",
              "description": "Forward body content including any additional commentary and the forwarded message content (plain text)",
              "type": "string",
              "required": true,
              "location": "body"
            },
            {
