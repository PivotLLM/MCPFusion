          "path": "/api/audits/{audit_id}",
          "parameters": [
            {
              "name": "audit_id",
              "description": "The audit ID (MongoDB ObjectId)",
              "type": "string",
              "required": true,
              "location": "path"
            }
          ],
          "response": { "type": "json" }
        },
        {
          "id": "create_audit",
          "name": "Create Audit",
          "description": "Create a new audit/pentest",
          "method": "POST",
